<header class="header">
    <div class="header__container container">
        <div class="header__body">
            <% if(typeof header_component.homePage !== 'undefined') { %>
                <%if(header_component.homePage === true) { %>
                    <%- include('externe/components/Image/Image.ejs', {
                        image_component: {
                            this: {
                                parent: 'header'
                            },
                            img: {
                                src: '/media/image/logo.svg',
                                alt: 'website-logo'
                            }
                        }
                    })%>
                <% } else if(header_component.homePage === false) { %>
                    <a href="./" class="header__link-logo">
                       <%- include('externe/components/Image/Image.ejs', {
                            image_component: {
                                block: 'header',
                                img: {
                                    src: '/media/image/logo.svg',
                                    alt: 'website-logo'
                                }
                            }
                        })%>
                    </a>
                <% } %>
            <% } %>
            <%- include('externe/components/HeaderMenu/HeaderMenu.ejs', {
                header_menu_component: {
                    activePage: header_component.active,
                    list: [
                        {'href': './services.html', 'anchor': 'Services'},
                        {'href': './work.html', 'anchor': 'Work'},
                        {'href': './about.html', 'anchor': 'About'},
                        {'href': './blog.html', 'anchor': 'Blog'},
                        {'anchor': 'Pages', 'submenus': [
                            {'href': '', 'anchor': 'Page1'},
                            {'href': '', 'anchor': 'Page2', 'submenus': [
                                {'href': '', 'anchor': 'Page2_1', 'submenus': [
                                    {'href': '', 'anchor': 'Page2_1_1'},
                                    {'href': '', 'anchor': 'Page2_1_1', 'submenus': [
                                        {'href': '', 'anchor': 'Page2_1_1_1'},
                                        {'href': '', 'anchor': 'Page2_1_1_2'},
                                    ]},
                                ]},
                                {'href': '', 'anchor': 'Page2_2', 'submenus': [
                                    {'href': '', 'anchor': 'Page2_2_1'},
                                    {'href': '', 'anchor': 'Page2_2_2'},
                                ]}
                            ]},
                            {'href': '', 'anchor': 'Page3', 'submenus': [
                                {'href': '', 'anchor': 'Page3_1', 'submenus': [
                                    {'href': '', 'anchor': 'Page3_1_1'},
                                    {'href': '', 'anchor': 'Page3_1_2'},
                                ]}
                            ]}
                        ], 'parent': 'Pages'},
                    ]
                }
            }) %>
            <%- include('src/ejs/components/button.ejs', {
                button_component: {
                    block: "header",
                    label: 'Letâ€™s talk',
                    link: '',
                    size: 'small',
                    style: 'primary'
                }
            })%>
            <%- include('externe/components/Burger/Burger.ejs', {
                burger_component: {
                    this: {
                        parent: 'header'
                    }
                }
            })%>
        </div>
    </div>
</header>